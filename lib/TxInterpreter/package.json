    "eslint": "^8.0.1",
    "jest": "^27.3.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.6",
    "typescript": "^4.4.4"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "lib",
    "src",
    "package.json",
    "tsconfig.json",
